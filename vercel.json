{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/cadastrar",
      "methods": [
        "POST"
      ],
      "dest": "/cadastrar.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://juliafullstack.site",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}